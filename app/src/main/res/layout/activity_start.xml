<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="madspild.Activities.StartActivity">
    <FrameLayout
        android:id="@+id/activity_start_loading"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/GS1LightGrayTransparent"
        android:clickable="true"
        android:focusable="true">
        <ProgressBar
            android:id="@+id/loading_spinner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:indeterminateTintMode="src_atop"
            android:indeterminateTint="@color/GS1Orange"
            android:layout_gravity="center" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/activity_start_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</FrameLayout>
